{
    "name": "eden159/tagging-bundle",
    "type": "symfony-bundle",
    "description": "Uses FabienPennequin/DoctrineExtensions-Taggable to add tagging to the Kunstmaan bundles",
    "keywords": [ "kunstmaan", "cms", "tag", "tagging"],
    "homepage": "https://github.com/eden159/KunstmaanTaggingBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Kunstmaan Team",
            "homepage": "http://www.kunstmaan.be/",
            "email": "support@kunstmaan.be"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "symfony/symfony": "~3.2",
        "fpn/doctrine-extensions-taggable": "0.9.0",
        "kunstmaan/admin-bundle": "~4.0",
        "kunstmaan/adminlist-bundle": "~4.0",
        "kunstmaan/media-bundle": "~4.0",
        "kunstmaan/node-bundle": "~4.0"
    },
    "autoload": {
        "psr-4": { "Kunstmaan\\TaggingBundle\\": "" }
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "4.0-dev"
        }
    }
}
